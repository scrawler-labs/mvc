{
    "name": "scrawler/mvc",
    "description": "use scrawler framework as mvc framework",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    
    "authors": [
        {
            "name": "ipranjal",
            "email": "pranjaltech10@gmail.com"
        }
    ],
    "require": {
        "scrawler/app": "^2.1",
        "scrawler/database": "^2.0",
        "scrawler/blade": "^2.0",
        "scrawler/storage": "^2.1",
        "vlucas/phpdotenv": "^5.6",
        "scrawler/exception": "^2.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "pestphp/pest": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
